{
  "expo": {
    "name": "pentia-chat",
    "slug": "pentia-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pc-icon.png",
    "userInterfaceStyle": "light",
    "scheme": "pentia-chat",
    "splash": {
      "image": "./assets/pc-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.buzzkill.pentia-chat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pc-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "app.buzzkill.pentia_chat"
    },
    "web": {
      "favicon": "./assets/pc-favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "553cbd0b-1502-4696-9cb7-c06477de6bfd"
      }
    }
  }
}
