{
  "expo": {
    "name": "pentia-chat",
    "slug": "pentia-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pc-icon.png",
    "userInterfaceStyle": "light",
    "scheme": "pentia-chat",
    "splash": {
      "image": "./assets/pc-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pc-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/pc-favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
